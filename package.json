{
  "name": "ember-bootstrap-codemods",
  "version": "0.1.3",
  "description": "Codemods to ease upgrading of ember-bootstrap",
  "keywords": [
    "codemod-cli"
  ],
  "repository": "https://github.com/kaliber5/ember-bootstrap-codemods",
  "license": "MIT",
  "author": "Simon Ihmig <simon.ihmig@kaliber5.de",
  "bin": "./bin/cli.js",
  "files": [
    "bin",
    "transforms"
  ],
  "scripts": {
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "lint": "eslint --cache .",
    "test": "codemod-cli test",
    "test:coverage": "codemod-cli test --coverage",
    "update-docs": "codemod-cli update-docs"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "codemod-cli": "^3.1.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.6",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "release-it": "^14.5.0",
    "release-it-lerna-changelog": "^3.1.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
