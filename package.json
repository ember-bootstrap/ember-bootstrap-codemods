{
  "name": "ember-bootstrap-codemods",
  "description": "Codemods to ease upgrading of ember-bootstrap",
  "version": "0.1.0",
  "author": "Simon Ihmig <simon.ihmig@kaliber5.de",
  "license": "MIT",
  "repository": "github:kaliber5/ember-bootstrap-codemods",
  "scripts": {
    "lint": "eslint --cache .",
    "test": "codemod-cli test",
    "test:coverage": "codemod-cli test --coverage",
    "update-docs": "codemod-cli update-docs",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "files": [
    "bin",
    "transforms"
  ],
  "bin": "./bin/cli.js",
  "keywords": [
    "codemod-cli"
  ],
  "dependencies": {
    "codemod-cli": "simonihmig/codemod-cli#template-support"
  },
  "devDependencies": {
    "coveralls": "^3.0.6",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^24.9.0",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
